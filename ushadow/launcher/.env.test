# Test Environment Configuration for Launcher
# Copy this to .env.local and modify values to test different scenarios

# Set to "true" to enable mock mode (no real Docker/Tailscale commands)
MOCK_MODE=false

# Mock prerequisite status
MOCK_DOCKER_INSTALLED=true
MOCK_DOCKER_RUNNING=false
MOCK_GIT_INSTALLED=true
MOCK_TAILSCALE_INSTALLED=false

# Mock container discovery
MOCK_INFRA_RUNNING=false
MOCK_ENVS_COUNT=0

# Platform override for testing (macos, windows, linux)
# MOCK_PLATFORM=macos
