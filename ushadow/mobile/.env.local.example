# Fastlane Environment Variables
# Copy this file to .env.local and fill in your values

# Your Apple ID (email address used for App Store Connect)
FASTLANE_APPLE_ID=your-apple-id@example.com

# Optional: App-specific password (if not using API key)
# Generate at: https://appleid.apple.com/account/manage → Security → App-Specific Passwords
# FASTLANE_APPLE_APPLICATION_SPECIFIC_PASSWORD=xxxx-xxxx-xxxx-xxxx

# Optional: Skip waiting for build processing (faster)
FASTLANE_SKIP_WAITING_FOR_BUILD_PROCESSING=true

# Optional: Session management
# FASTLANE_SESSION=<session_token>  # Auto-generated on first login

# Optional: Disable analytics
FASTLANE_OPT_OUT_USAGE=1
